{
    "cli": {
      "version": ">= 10.0.0"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      },
      "preview": {
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        }
      },
      "production": {
        "autoIncrement": true,
        "android": {
          "buildType": "app-bundle"
        },
        "ios": {
          "simulator": false
        }
      },
      "stealth": {
        "autoIncrement": true,
        "env": {
          "STEALTH": "true"
        },
        "android": {
          "buildType": "apk"
        },
        "ios": {
          "simulator": false
        }
      }
    },
    "submit": {
      "production": {}
    }
  }
  