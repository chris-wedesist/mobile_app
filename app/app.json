{
  "expo": {
    "name": "DESIST",
    "slug": "desist",
    "version": "1.0.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "desist",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-dev-client"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_GOOGLE_PLACES_API_KEY": "your_api_key_here",
      "EXPO_PUBLIC_GOOGLE_SEARCH_API_KEY": "your_search_api_key_here",
      "EXPO_PUBLIC_GOOGLE_SEARCH_ENGINE_ID": "your_search_engine_id_here"
    }
  }
}
